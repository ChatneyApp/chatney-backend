type ChannelGroup {
    Id: ID!
    name: String!
    channels: [ID!]!
    order: Int!
    workspace: ID!
}

input CreateChannelGroupInput {
    name: String!
    channels: [ID!]!
    order: Int!
    workspace: ID!
}

input UpdateChannelGroupInput {
    Id: ID!
    name: String
    channels: [ID!]
    order: Int
}