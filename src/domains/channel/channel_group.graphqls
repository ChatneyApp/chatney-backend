type ChannelGroup {
    Id: ID!
    Name: String!
    Channels: [ID!]!
    Order: Int!
    Workspace: ID!
    CreatedAt: Time!
    UpdatedAt: Time!
}

input CreateChannelGroupInput {
    Name: String!
    Channels: [ID!]!
    Order: Int!
    Workspace: ID!
}

input UpdateChannelGroupInput {
    Id: ID!
    Name: String
    Channels: [ID!]
    Order: Int
}